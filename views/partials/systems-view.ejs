<div class="container-fluid">
    <div class="row">
        <div class="col-md-10">
            <!-- Body content -->
            <ul class="systems">
                <li data-ng-repeat="system in systems"
                    class="thumbnail system-listing">
                    <a href="#/systems/{{system._id}}" class="thumb"> System ID :{{ system.id }}</a>
                    <p> Duration : {{system.duration}}</p>
                </li>
            </ul>
        </div>
    </div>
</div>